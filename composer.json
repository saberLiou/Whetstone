{
    "name": "saberliou/whetstone",
    "description": "A Laravel package with console commands like `php artisan make` for generating self-defined PHP blank classes.",
    "license": "MIT",
    "authors": [
        {
            "name": "saberLiou",
            "email": "saberliou@gmail.com",
            "homepage": "https://github.com/saberLiou"
        }
    ],
    "homepage": "https://github.com/saberliou/whetstone",
    "keywords": ["Laravel", "Whetstone"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "saberLiou\\Whetstone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "saberLiou\\Whetstone\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "saberLiou\\Whetstone\\WhetstoneServiceProvider"
            ],
            "aliases": {
                "Whetstone": "saberLiou\\Whetstone\\Facades\\Whetstone"
            }
        }
    }
}
